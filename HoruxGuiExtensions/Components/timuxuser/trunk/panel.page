												<img src="<%~ ./assets/mybooking.png %>" alt="<%[My sign in/out]%>"  />
												<span><%[My sign in/out]%></span>
											</a>
										</div>
									</div>
									</prop:TrueTemplate>
									</com:TConditional>

									<com:TConditional Condition="$this->isAccess('components.timuxuser.leaverequest.leaverequest')">
