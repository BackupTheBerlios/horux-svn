<fieldset class="adminform">
  <legend><%[Horux Core Information]%></legend>

  <table class="adminlist">
    <thead>
        <tr>
            <th width="250">
                <%[Setting]%>
            </th>
            <th>
                <%[Value]%>
            </th>
        </tr>
    </thead>
    <tfoot>
    <tr>
        <th colspan="2">
            &nbsp;
        </th>
    </tr>
    </tfoot>
    <tbody>

    <tr>
        <td>
            <%[Horux Core Version]%>:
        </td>
        <td>
        	<com:TLabel id="horuxVersion" />
        </td>
    </tr>

    <tr>
        <td>
            <%[Horux time live]%>:
        </td>
        <td>
        	<com:TLabel id="horuxTimeLive" />
        </td>
    </tr>
    
    </tbody>
  </table>    
  
</fieldset>